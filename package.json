{
  "name": "kosen-fes-2025",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && node scripts/generate-sitemap.js && node scripts/generate-static.js",
    "build:static": "tsc -b && vite build && node scripts/generate-static.js",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --apply .",
    "typecheck": "tsc -b --pretty false",
    "build:silent": "tsc -b --pretty false && vite build --logLevel silent",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "date-fns": "^4.1.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.545.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "tailwindcss": "^4.1.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^27",
    "@types/node": "^24.0.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "jsdom": "^27.0.1",
    "terser": "^5.44.0",
    "typescript": "~5.3.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@9.0.0"
}